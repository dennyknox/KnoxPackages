  RowBox[{"f", "[", "x_", "]"}], ":=", 
  RowBox[{"\[Piecewise]", GridBox[{
     {
      SuperscriptBox["x", "3"], 
      RowBox[{"x", "<", "0"}]},
     {
      RowBox[{
       RowBox[{"Sin", "[", 
        RowBox[{"4", "x"}], "]"}], "+", "1"}], 
      RowBox[{"0", "\[LessEqual]", "x", "<", 
       FractionBox["\[Pi]", "4"]}]},

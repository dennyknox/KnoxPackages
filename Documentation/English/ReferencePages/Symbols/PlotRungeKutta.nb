      RowBox[{
       SuperscriptBox["z", "\[Prime]",
        MultilineFunction->None], "[", "t", "]"}], "==", 
      RowBox[{"2", "-", 
       RowBox[{
        RowBox[{"z", "[", "t", "]"}], " ", 
        RowBox[{"(", 
         RowBox[{"4", "-", 
          RowBox[{"x", "[", "t", "]"}]}], ")"}]}]}]}], ",", 
     RowBox[{
      RowBox[{"z", "[", "0", "]"}], "==", "0.52"}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"x", "[", "t", "]"}], ",", 
     RowBox[{"y", "[", "t", "]"}], ",", 
     RowBox[{"z", "[", "t", "]"}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"t", ",", "0", ",", "25", ",", ".2"}], "}"}], ",", 
   RowBox[{"PointStyle", "\[Rule]", 
    RowBox[{"AbsolutePointSize", "[", "1.25", "]"}]}], ",", 

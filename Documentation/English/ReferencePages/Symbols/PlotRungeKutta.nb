  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       SuperscriptBox["x", "\[Prime]",
        MultilineFunction->None], "[", "t", "]"}], "==", 
      RowBox[{
       RowBox[{"-", 
        RowBox[{"y", "[", "t", "]"}]}], "-", 
       RowBox[{"z", "[", "t", "]"}]}]}], ",", 
     RowBox[{
      RowBox[{"x", "[", "0", "]"}], "==", 
      RowBox[{"-", "0.04"}]}], ",", 
     RowBox[{
      RowBox[{
       SuperscriptBox["y", "\[Prime]",
        MultilineFunction->None], "[", "t", "]"}], "==", 
      RowBox[{
       RowBox[{"x", "[", "t", "]"}], "+", 
       RowBox[{".425", " ", 

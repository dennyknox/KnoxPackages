 RowBox[{"Needs", "[", "\"\<KnoxPackages`Calculus`\>\"", 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{"Clear", "[", "f", "]"}], "\n", 
 RowBox[{
  RowBox[{"f", "[", "t_", "]"}], ":=", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{
     RowBox[{"Sin", "[", "t", "]"}], " ", 
     RowBox[{"Cos", "[", 
      RowBox[{"3", " ", "t"}], "]"}]}], ",", 
    RowBox[{"Sin", "[", 
     RowBox[{"3", " ", "t"}], "]"}]}], "}"}]}], "\n", 
 RowBox[{"ParaPlotTangentLine", "[", 
  RowBox[{
   RowBox[{"f", "[", "t", "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"t", ",", "0", ",", 
     RowBox[{"2", " ", "\[Pi]"}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"-", "1.2"}], ",", ".20", ",", "1.2"}], "}"}], ",", 
   RowBox[{"TangentLineStyle", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"Thick", ",", "Red"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"Thick", ",", "Black"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"Thick", ",", "Purple"}], "}"}]}], "}"}]}], ",", 
   RowBox[{"LineLength", "\[Rule]", "1.5"}], ",", 
   RowBox[{"ColorFunction", "\[Rule]", "Automatic"}], ",", 
   RowBox[{"DrawArrowheads", "\[Rule]", "True"}]}], "]"}]}], "Input",

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{"Clear", "[", "f", "]"}], "\n", 
 RowBox[{
  RowBox[{"f", "[", "x_", "]"}], ":=", 
  RowBox[{"\[Piecewise]", GridBox[{
     {
      SuperscriptBox["x", 
       RowBox[{"5", "/", "3"}]], 
      RowBox[{"x", "<", "0"}]},
     {
      RowBox[{
       RowBox[{"Sin", "[", 
        RowBox[{"4", " ", "x"}], "]"}], "+", "1"}], 
      RowBox[{"0", "\[LessEqual]", "x", "<", "1"}]},
     {"4", 
      RowBox[{"x", "\[Equal]", "1"}]},

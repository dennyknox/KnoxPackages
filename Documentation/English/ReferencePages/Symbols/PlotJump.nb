Cell["\<\
Here are two functions with jump discontinuities with one function having an \
asymptote.\
\>", "ExampleText",
 CellID->825254162,ExpressionUUID->"57a13879-f376-4016-afe3-b2729048f176"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{"Needs", "[", "\"\<KnoxPackages`Calculus`\>\"", 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"f", "[", "x_", "]"}], ":=", 
  RowBox[{"\[Piecewise]", GridBox[{
     {
      RowBox[{"2", "-", "x"}], 
      RowBox[{"x", "<", 
       RowBox[{"-", "1"}]}]},
     {
      RowBox[{"Sin", "[", 
       RowBox[{"4", " ", "x"}], "]"}], 
      RowBox[{
       RowBox[{"-", "1"}], "\[LessEqual]", "x", "<", "1"}]},
     {"4", 
      RowBox[{"x", "==", "1"}]},
     {
      RowBox[{"4", "-", "x"}], 
      RowBox[{"x", ">", "1"}]}
    }]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"g", "[", "x_", "]"}], ":=", 
  RowBox[{"\[Piecewise]", GridBox[{
     {
      RowBox[{"-", 
       FractionBox["1", 
        RowBox[{"x", "+", "1"}]]}], 
      RowBox[{"x", "<", 
       RowBox[{"-", "1"}]}]},
     {
      RowBox[{"Cos", "[", 
       RowBox[{"5", " ", "x"}], "]"}], 
      RowBox[{
       RowBox[{"-", "1"}], "\[LessEqual]", "x", "<", "1"}]},
     {"5", 
      RowBox[{"x", "\[Equal]", "1"}]},
     {
      SuperscriptBox["x", "2"], 
      RowBox[{"x", ">", "1"}]}
    }]}]}], "\[IndentingNewLine]", 
 RowBox[{"PlotJump", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"f", "[", "x", "]"}], ",", 
     RowBox[{"g", "[", "x", "]"}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", 
     RowBox[{"-", "2"}], ",", "2"}], "}"}], ",", 
   RowBox[{"Jump", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"-", "1"}], ",", "1"}], "}"}]}], ",", 
   RowBox[{"Asymptote", "\[Rule]", 
    RowBox[{"-", "1"}]}], ",", 
   RowBox[{"PlotStyle", "->", 
    RowBox[{"{", 
     RowBox[{"Blue", ",", "Red"}], "}"}]}], ",", 
   RowBox[{"ImageSize", "\[Rule]", "Medium"}]}], "]"}]}], "Input",
 CellLabel->"In[1]:=",
 CellID->114202103,ExpressionUUID->"1734ca37-6ac9-4876-9f19-3c80d25cf3e9"],


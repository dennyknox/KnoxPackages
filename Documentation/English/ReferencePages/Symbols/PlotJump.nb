Cell[TextData[{
 "By default, when ",
 Cell[BoxData[
  TemplateBox[{Cell[
     TextData["PlotJump"]],"paclet:KnoxPackages/ref/PlotJump"},
   "RefLink",
   BaseStyle->{"InlineFormula"}]], "InlineFormula",ExpressionUUID->
  "51a2e911-2cb2-4d35-af67-4401d187b458"],
 " is given a piecewise function, it changes the default behavior of ",
 Cell[BoxData[
  TemplateBox[{Cell[
     TextData["Piecewise"]],"paclet:ref/Piecewise"},
   "RefLink",
   BaseStyle->{"InlineFormula"}]], "InlineFormula",ExpressionUUID->
  "2cc82d61-afd5-4c5d-a9fd-4f6a3dfded26"],
 " so that it does not default to a value of 0. This allows ",
 Cell[BoxData[
  TemplateBox[{Cell[
     TextData["PlotJump"]],"paclet:KnoxPackages/ref/PlotJump"},
   "RefLink",
   BaseStyle->{"InlineFormula"}]], "InlineFormula",ExpressionUUID->
  "3fca78bf-8e1b-4409-b164-8c1da9500cfe"],
 " to give a correct plot of this function."
}], "ExampleText",
 CellID->61957931,ExpressionUUID->"59b12424-078d-48b7-ade3-96b48ec1bae8"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{"Needs", "[", "\"\<KnoxPackages`Calculus`\>\"", 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"f", "[", "x_", "]"}], ":=", 
  RowBox[{"\[Piecewise]", GridBox[{
     {
      SuperscriptBox["x", "2"], 
      RowBox[{"x", "\[LessEqual]", "1"}]},
     {
      RowBox[{
       SuperscriptBox["x", "2"], "+", "1"}], 
      RowBox[{"x", "\[GreaterEqual]", "2"}]}
    }]}]}], "\[IndentingNewLine]", 
 RowBox[{"PlotJump", "[", 
  RowBox[{
   RowBox[{"x", "+", 
    RowBox[{"f", "[", "x", "]"}]}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", 
     RowBox[{"-", "1"}], ",", "3"}], "}"}], ",", 
   RowBox[{"Jump", "->", 
    RowBox[{"{", 
     RowBox[{"1", ",", "2"}], "}"}]}]}], "]"}]}], "Input",
 CellLabel->"In[1]:=",
 CellID->248195692,ExpressionUUID->"089030c8-37ec-4866-a707-74bcd41a3903"],


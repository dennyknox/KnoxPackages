Cell["A function with jump discontinuities:", "ExampleText",
 CellID->627150712,ExpressionUUID->"a60c884c-fc5f-44f8-afb2-e12b7b5705e4"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{"Needs", "[", "\"\<KnoxPackages`Calculus`\>\"", 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{"Clear", "[", "f", "]"}], "\n", 
 RowBox[{
  RowBox[{"f", "[", "x_", "]"}], ":=", 
  RowBox[{"\[Piecewise]", GridBox[{
     {
      SuperscriptBox["x", 
       RowBox[{"1", "/", "3"}]], 
      RowBox[{"x", "<", "1"}]},
     {
      RowBox[{"-", "1"}], 
      RowBox[{"x", "==", "1"}]},
     {
      RowBox[{
       SuperscriptBox[
        RowBox[{"Sin", "[", "x", "]"}], "2"], "+", 
       RowBox[{"3", " ", 
        RowBox[{"Cos", "[", "x", "]"}]}]}], "True"}
    }]}]}], "\[IndentingNewLine]", 
 RowBox[{"PlotProperty", "[", 
  RowBox[{
   RowBox[{"f", "[", "x", "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", 
     RowBox[{"-", "3"}], ",", "4"}], "}"}], ",", "Negative", ",", 
   RowBox[{"Jump", "\[Rule]", "1"}], ",", 
   RowBox[{"PowerBehavior", "\[Rule]", "Real"}], ",", 
   RowBox[{"ImageSize", "\[Rule]", "Medium"}]}], "]"}]}], "Input",
 CellLabel->"In[2]:=",
